{
	"extends": "@docusaurus/tsconfig",
	"compilerOptions": {
		"jsx": "react-jsx",
		"baseUrl": ".",
		"skipLibCheck": true,
		"resolveJsonModule": true
	},
	"exclude": [
		"**/*.test.ts",
		"**/*.test.tsx",
		"**/*.spec.ts",
		"**/*.spec.tsx",
		"**/__tests__/**/*",
		"tests/**/*",
		"functions/**/*",
		"src/utils/typescript/**"
	]
}
